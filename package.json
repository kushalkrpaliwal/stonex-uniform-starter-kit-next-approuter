{
  "name": "component-starter-kit-rsc",
  "version": "5.2.1",
  "private": true,
  "engines": {
    "npm": ">=8.19.0",
    "node": ">=18.17.0",
    "yarn": "please-use-npm",
    "pnpm": "please-use-npm"
  },
  "scripts": {
    "build-utilities": "node src/tokens/build.mjs",
    "update-figma-tokens": "cd src/figma && git pull origin main && npm run build-utilities",
    "dev": "run-s uniform:manifest:pull dev:next",
    "dev:next": "next dev",
    "build:next": "next build",
    "build": "run-s uniform:manifest:pull build:next",
    "build:test": "export NEXT_PUBLIC_E2E_TEST=true && run-s uniform:manifest:pull build:next",
    "start": "next start",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,scss}\"",
    "uniform:pull": "uniform sync pull",
    "uniform:push": "uniform sync push",
    "uniform:manifest:pull": "uniform context manifest download --output ./src/context/manifest.json",
    "uniform:manifest:publish": "uniform context manifest publish",
    "postinstall": "cd cli-run && npm install && npm run build",
    "cli": "npm i && cd cli-run && node dist/index.mjs",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@carbon/icons": "^11.43.0",
    "@carbon/icons-react": "^11.43.0",
    "@contentful/rich-text-html-renderer": "^16.1.0",
    "@contentful/rich-text-types": "^16.2.0",
    "@tokens-studio/sd-transforms": "^0.14.4",
    "@uniformdev/canvas-next-rsc": "^19.161.0",
    "@uniformdev/context-next": "^19.161.0",
    "@uniformdev/project-map": "^19.161.0",
    "@uniformdev/richtext": "^19.161.0",
    "@vercel/og": "^0.5.7",
    "classnames": "^2.3.2",
    "daisyui": "^3.9.3",
    "framer-motion": "^10.16.2",
    "next": "^14.2.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-multi-carousel": "^2.8.3",
    "react-player": "^2.12.0",
    "react-responsive-masonry": "^2.1.7",
    "sass": "^1.64.1",
    "sharp": "^0.32.1",
    "style-dictionary": "^3.9.2"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.4.0",
    "@storybook/addon-essentials": "^8.1.6",
    "@storybook/addon-interactions": "^8.1.6",
    "@storybook/addon-onboarding": "^8.1.6",
    "@storybook/addon-themes": "^8.1.6",
    "@storybook/blocks": "^8.1.6",
    "@storybook/nextjs": "^8.1.6",
    "@storybook/react": "^8.1.6",
    "@storybook/test": "^8.1.6",
    "@tailwindcss/typography": "^0.5.9",
    "@types/color": "^3.0.3",
    "@types/node": "^18.15.11",
    "@types/node-fetch": "^2.6.4",
    "@types/react": "18.2.45",
    "@types/react-dom": "18.2.18",
    "@types/react-responsive-masonry": "2.1.0",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "@typescript-eslint/types": "^6.4.0",
    "@uniformdev/cli": "^19.161.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.47.0",
    "eslint-config-custom": "*",
    "eslint-config-next": "^13.5.6",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-storybook": "^0.8.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.23",
    "postcss-import": "^16.1.0",
    "postcss-nested": "^6.0.1",
    "prettier": "^3.0.2",
    "shared": "*",
    "storybook": "^8.1.6",
    "storybook-addon-data-theme-switcher": "^0.4.2",
    "tailwindcss": "^3.3.1",
    "ts-node": "^10.9.1",
    "typescript": "5.3.3"
  }
}
